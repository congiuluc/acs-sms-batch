# Environment Variables Configuration

# Azure Communication Services
export ACS_CONNECTION_STRING="endpoint=https://your-acs-resource.communication.azure.com/;accesskey=your-access-key"
export ACS_FROM_PHONE_NUMBER="+12345678901"

# SMS Configuration
export SMS_MESSAGE_TEMPLATE="Hello {DisplayName}, this is a message from Azure Communication Services."

# Rate Limiting (adjust based on your number type)
export RATE_LIMIT_REQUESTS_PER_MINUTE=200
export RATE_LIMIT_BATCH_SIZE=50
export RATE_LIMIT_MAX_CONCURRENT=10

# CSV Configuration
export CSV_FILE_PATH="sample.csv"
export CSV_PHONE_COLUMN="AuthMethodMobileNumbers"
export CSV_NAME_COLUMN="DisplayName"
